  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch qemu --help",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../install/bin/qemu-system-gnuarmeclipse",
      "args": [
        "--help"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
    {
      "name": "(lldb) Launch qemu f4b-sh --nographic",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../install/bin/qemu-system-gnuarmeclipse",
      "args": [
        "--verbose",
        "--verbose",
        "--board",
        "STM32F4-Discovery",
        "--mcu",
        "STM32F407VG",
        "-d",
        "unimp,guest_errors",
        "--nographic",
        "--image",
        "${env:HOME}/Work/f4b-sh/Debug/f4b-sh.elf",
        "--semihosting-config",
        "enable=on,target=native",
        "--semihosting-cmdline",
        "f4b",
        "20"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "lldb"
    },
    {
      "name": "(lldb) Launch qemu f4b-sh (graphic)",
